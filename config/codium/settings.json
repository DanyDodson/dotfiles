{
  "update.mode": "manual",
  //
  "security.workspace.trust.enabled": false,
  //
  "telemetry.telemetryLevel": "off",
  // 
  "redhat.telemetry.enabled": false,
  // 
  "continue.enableQuickActions": true,
  "continue.showInlineTip": true,
  "continue.telemetryEnabled": false,
  "continue.enableTabAutocomplete": false,
  // 
  "code-runner.enableAppInsights": false,
  //
  "git.confirmSync": false,
  "git.enableStatusBarSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.defaultCloneDirectory": "/Users/dany/repos",
  "git.decorations.enabled": true,
  // 
  "github.gitProtocol": "ssh",
  //
  "scm.countBadge": "off",
  "scm.repositories.visible": 0,
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterWidth": 1,
  "scm.diffDecorationsGutterVisibility": "hover",
  // 
  "sync.autoUpload": false,
  "sync.forceUpload": false,
  "sync.quietSync": false,
  "sync.gist": "71825053bddd9002c55a15962d1804cf",
  //
  "autoOpenPreviewPanel.languages": "markdown,asciidoc",
  // 
  "Lua.runtime.version": "Lua 5.1",
  //
  "breadcrumbs.enabled": false,
  //
  "npm-intellisense.importLinebreak": "",
  "npm-intellisense.packageSubfoldersIntellisense": true,
  "npm-intellisense.scanDevDependencies": true,
  //
  "liveServer.settings.donotShowInfoMsg": true,
  //
  "prettier.tabWidth": 2,
  "prettier.semi": true,
  //
  "color-highlight.markRuler": false,
  // 
  "explorer.incrementalNaming": "smart",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "explorer.decorations.badges": false,
  //
  "window.zoomLevel": 0.4,
  "window.commandCenter": false,
  "window.nativeFullScreen": false,
  "window.titleBarStyle": "native",
  "window.density.editorTabHeight": "compact",
  "window.confirmSaveUntitledWorkspace": false,
  "workbench.startupEditor": "none",
  "workbench.panel.showLabels": false,
  "workbench.layoutControl.enabled": false,
  "workbench.list.fastScrollSensitivity": 5,
  "workbench.tree.enableStickyScroll": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.settings.editor": "json",
  "workbench.editor.decorations.badges": false,
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.statusBar.visible": false,
  //
  "editor.codeLens": false,
  "editor.autoClosingQuotes": "never",
  "editor.autoClosingBrackets": "never",
  "editor.fontSize": 14.3,
  "editor.lineHeight": 26,
  "editor.tabSize": 2,
  "editor.folding": false,
  "editor.padding.top": 15,
  "editor.padding.bottom": 15,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.renderLineHighlight": "none",
  "editor.glyphMargin": false,
  "editor.lineNumbers": "relative",
  "editor.lineDecorationsWidth": 12,
  "editor.stickyScroll.enabled": false,
  "editor.fontWeight": 400,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 8,
  //
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  //
  "extensions.ignoreRecommendations": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  //
  "diffEditor.wordWrap": "off",
  "diffEditor.renderGutterMenu": true,
  //
  "debug.toolBarLocation": "docked",
  "debug.console.wordWrap": false,
  "debug.javascript.codelens.npmScripts": "never",
  //
  "shellformat.path": "/opt/homebrew/bin/shfmt",
  //
  "files.associations": {
    "gitlocal": "properties",
    "gitalias": "",
    "gitsecret": "properties",
    "*tmpl": "html",
    "dircolors*": "dircolors"
  },
  // 
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "go.enableCodeLens": {
    "runtest": false
  },
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.wrapLineLength": 9999,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "javascript.format.semicolons": "insert",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "json.format.keepLines": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "typescript.format.semicolons": "insert",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[xml]": {
    "breadcrumbs.showArrays": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
  },
  "zig.zls.enabled": "on",
  //
  "apc.electron": {
    "frame": false
  },
  "apc.imports": [
    "${userHome}/.dotfiles/.config/apc/main.js",
    "${userHome}/.dotfiles/config/apc/styles.css"
  ],
  "apc.statusBar": {
    "fontSize": 10.5,
    "height": 18,
  },
  "apc.activityBar": {
    "size": 30,
    "itemSize": 26,
    "itemMargin": 10,
  },
  // 
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": false,
      "values": [ { "editor.lineNumbers": "relative" }, { "editor.lineNumbers": "off" } ]
    },
    {
      "id": "continueCompletions",
      "overrideWorkspaceSettings": false,
      "values": [ { "continue.enableTabAutocomplete": true }, { "continue.enableTabAutocomplete": false } ]
    }
  ],
  // 
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  //
  "workbench.colorCustomizations": {
    "editorLineNumber.foreground": "#71717178",
    "statusBarItem.remoteBackground": "#ff000000",
    "statusBarItem.remoteForeground": "#7e8185",
    "statusBar.foreground": "#7e8185",
    "activityBar.background": "#22262d",
    "statusBar.focusBorder": "#ffffff00",
    "focusBorder": "#ffffff00"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "meta.var.expr storage.type",
          "keyword.control.flow",
          "keyword.control.return",
          "meta.directive.vue punctuation.separator.key-value.html",
          "meta.directive.vue entity.other.attribute-name.html",
          "tag.decorator.js entity.name.tag.js",
          "tag.decorator.js punctuation.definition.tag.js",
          "storage.modifier"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [ "comment keyword.codetag.notation", "comment.block.documentation keyword", "comment.block.documentation storage.type.class" ],
        "settings": { "foreground": "#bb9af7" }
      },
      {
        "scope": [ "comment.block.documentation entity.name.type.instance" ],
        "settings": { "foreground": "#73daca", "fontStyle": "italic" }
      },
      {
        "scope": [ "comment.block.documentation entity.name.type punctuation.definition.bracket" ],
        "settings": { "foreground": "#bb9af7" }
      },
      {
        "scope": [ "comment.block.documentation variable" ],
        "settings": { "foreground": "#e0af68", "fontStyle": "italic" }
      }
    ]
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "Cargo.toml": "Cargo.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  }
}
