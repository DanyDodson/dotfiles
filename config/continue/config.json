    "apiBase": "https://api.mistral.ai/v1"
  },
  "tabAutocompleteOptions": {
    "useCopyBuffer": false,
    "useFileSuffix": true,
    "prefixPercentage": 0.7,
    "debounceDelay": 500,
    "maxPromptTokens": 1500,
    "multilineCompletions": "always",
    "onlyMyCode": true,
    "useOtherFiles": true,
    "disableInFiles": [ "*.md", ".csv" ]
  },
  "contextProviders": [
    { "name": "file" },
    { "name": "code" },
    { "name": "diff" },
    { "name": "search" },
